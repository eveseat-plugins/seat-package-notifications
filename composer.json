{
    "name": "recursivetree/seat-package-notifications",
    "description": "Send notifications when packages are outdated.",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "recurisvetree",
            "email": "recursivetreemail@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "RecursiveTree\\Seat\\PackageNotifications\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "eveseat/web": "^5.0",
        "evseat/services": "^5.0",
        "eveseat/notifications": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RecursiveTree\\Seat\\PackageNotifications\\PackageNotificationsServiceProvider"
            ]
        }
    }
}
